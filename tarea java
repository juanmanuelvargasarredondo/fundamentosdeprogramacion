 clase  pública Beca {
  Escáner estático sc =  nuevo  escáner ( System . in);


 becaPresEst vacío  estático () {
    // Declaracion de variables e inicializacion
    int edad = 0 ;
    doble promedio = 0 , montoBeca = 0 ;
    // Datos de Entrada
    Sistema . fuera . println ( " Ingrese su edad: " );
    edad = sc . nextInt ();
    Sistema . fuera . println ( " Ingrese su promedio de nota: " );
    promedio = sc . nextDouble ();    
    // Proceso
    if (edad > 18 ) {
      si (promedio > = 9 ) {
        montoBeca = 2000 ;
      } más  si (promedio > = 7.5  && promedio < 9 ) {
        montoBeca = 1000 ;
      } más  si (promedio > = 6  && promedio < 7.5 ) {
        montoBeca = 500 ;
      } más {
        montoBeca = 0 ;
      }
    } más {
      si (promedio > = 9 ) {
        montoBeca = 3000 ;
      } más  si (promedio > = 8  && promedio < 9 ) {
        montoBeca = 2000 ;
      } más  si (promedio < 8  && promedio > = 6 ) {
        montoBeca = 100 ;
      } más {
        montoBeca = 0 ;
      }
    }
    // Datos Salida
    if (montoBeca == 0 ) {
      Sistema . fuera . println ( "¡ Esforzarse para el próximo ciclo ...! " );
    } más {
      Sistema . fuera . println ( " Felicitaciones Ud. es acreedor de una Beca por el Monto de: $ " + montoBeca);
    }

}

static  void  mostrarDiaLetras () {
  // Definir variables
  int dia = 0 ;
  String nombredia = " " ;
  // Datos de Entrada
  Sistema . fuera . println ( " Ingrese un día de la semana entre (1-7): " );
  dia = sc . nextInt ();
  // Proceso
  switch (dia) {
    caso  1 : nombredia = " Domingo " ; romper ;
    caso  2 : nombredia = " Lunes " ; romper ;
    caso  3 : nombredia = " Martes " ; romper ;
    caso  4 : nombredia = " Miercoles " ; romper ;
    caso  5 : nombredia = " Jueves " ; romper ;
    caso  6 : nombredia = " Viernes " ; romper ;
    caso  7 : nombredia = " Sábado " ; romper ;
    por defecto :
      nombredia = " No existe " ;
  }
  // Datos de Salida
  Sistema . fuera . println ( " El dia es: " + nombredia);
}

static  void  suma20mumerosPares () {
  // Declarar variables e inicializacion de valores
  int numInit = 1 , sumaNumPar = 0 ;
  // Proceso
    while (numInit <=  20 ) {
      if (numInit % 2 == 0 ) {
        sumaNumPar = sumaNumPar + numInit;
      }
      numInit ++ ; // numInit = numInit + 1;
    }
  // Datos de Salida
    Sistema . fuera . println ( " La suma de números pares entre 1 y 20 es: " + sumaNumPar);
}

static  void  menuOpciones () {
  int opcionesA = 0 ;
  Sistema . fuera . println ( " ------------------------------- " );
  String msg = " \ n Ingrese el Algoritmo que desea Probar 1 = xxxx, 2 = xxxx, 3 = numeros pares: " ;
  Sistema . fuera . println (mensaje);
  opcionesA = sc . nextInt ();  
  while (opcionesA ! = 0 ) {
      cambiar (opcionesA) {
        caso  1 : becaPresEst (); romper ;
        caso  2 : mostrarDiaLetras (); romper ;
        caso  3 : suma20mumerosPares (); romper ;
        caso  4 : personaMenor (); romper ;
        por defecto :
          Sistema . fuera . println ( " El Algoritmo no existe! " );
      }   
    Sistema . fuera . println ( " ------------------------------ " );  
    Sistema . fuera . println (mensaje);
    opcionesA = sc . nextInt ();        
  }
}

static  void  personaMenor () {
  // declarar variables
  Cadena nomA, nomB, nomC, nomM = " " ;
  int edadA, edadB, edadC, edadM = 0 ;
  // Datos de Entrada
  Sistema . fuera . println ( " Ingrese nombre 1ra Persona: " );
  nomA = sc . Siguiente();
  Sistema . fuera . println ( " Ingrese edad 1ra Persona: " );
  edadA = sc . nextInt ();  
  Sistema . fuera . println ( " Ingrese nombre 2da Persona: " );
  nomB = sc . Siguiente();
  Sistema . fuera . println ( " Ingrese edad 2da Persona: " );
  edadB = sc . nextInt ();  
  Sistema . fuera . println ( " Ingrese nombre 3ra Persona: " );
  nomC = sc . Siguiente();
  Sistema . fuera . println ( " Ingrese edad 3ra Persona: " );
  edadC = sc . nextInt ();      
  // Proceso
  if (edadA < edadB && edadA < edadC) {
    edadM = edadA;
    nomM = nomA;
  }
if (edadB < edadA && edadB < edadC) {
    edadM = edadB;
    nomM = nomB;
  }
if (edadC < edadA && edadC < edadB) {
    edadM = edadC;
    nomM = nomC;
  }    
  // Datos de salida
  Sistema . fuera . println ( " La persona menor es: " + nomM + " y su edad es: " + edadM);
}



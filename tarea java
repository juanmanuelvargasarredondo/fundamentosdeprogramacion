public  class  PagoservTelef {
   Escáner estático sc =  nuevo  escáner ( System . in);

   practica vacío  estático () {
    Sistema . fuera . println ( " Ingrese su nombre: " );
    Cadena nombre = sc . Siguiente();
    Sistema . fuera . println ( " Ingrese su edad: " );
    int edad = sc . nextInt ();
    Sistema . fuera . println ( " Nombre ingresado es: " + nombre + " y la edad es: " + edad);
  }

  static  void  pagoservicioTelef () {
    // Definir variables
    String dia = " " , turno = " " ;
    int minL = 0 ;
    doble pagoL = 0 , pImpuesto = 0 , totalPago = 0 ;
    // Datos de entrada
    Sistema . fuera . print ( " Ingrese Min de llamada: " );
    minL = sc . nextInt ();
    Sistema . fuera . print ( " \ n Dia de llamada: " );
    dia = sc . siguiente () . toUpperCase ();
    Sistema . fuera . print ( " \ n Turno de llamada: " );
    turno = sc . siguiente () . toUpperCase ();    
    // Proceso
    si (minL <= 5 ) {
      pagoL = minL * 1 ;
    } más  si (minL > 5  && minL <= 8 ) {
      pagoL = ( 5 * 1 ) + ((minL - 5 ) * 0.8 );
    } más  si (minL > 8  && minL <= 10 ) {
      pagoL = ( 5 * 1 ) + ( 3 * 0.8 ) + ((minL - 8 ) * 0.7 );
    } más {
      pagoL = ( 5 * 1 ) + ( 3 * 0.8 ) + ( 2 * 0.7 ) + ((minL - 10 ) * 0.5 );
    }
    if (dia . equals ( " D " ) || dia . equals ( " S " )) { // dias no habiles
        si (dia . es igual ( " D " )) {
          pImpuesto = pagoL * 0.03 ;
        }
    } else { // Dias habiles
      if (turno . es igual a ( " M " )) {
        pImpuesto = pagoL * 0.15 ;
      } más {
        pImpuesto = pagoL * 0.10 ;
      }
    }
    totalPago = pagoL + pImpuesto;
    // Datos de salida
    Sistema . fuera . println ( " Pago por llamada: " + pagoL);
    Sistema . fuera . println ( " Impuesto: " + pImpuesto);
    Sistema . fuera . println ( " Pago total: " + pago total);
  }


  public  static  void  main ( String [] args ) {
    pagoservicioTelef ();

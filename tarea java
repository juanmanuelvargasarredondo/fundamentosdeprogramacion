 clase  pública Banco_GFYM {

    public  static  void  main ( String [] args ) {
        Escáner en =  nuevo  escáner ( Sistema . En);
        doble compras_realizadas, intereses, multa, pago_del_corte_anterior, pago_minimo;
        doble pago_para_no_generar_intereses, saldo_actual, saldo_anterior;
        Sistema . fuera . print ( " Ingresa el valor de compras realizadas: " );
        compras_realizadas = en . nextDouble ();
        en . línea siguiente();
        Sistema . fuera . print ( " Ingresa el valor de pago del corte anterior: " );
        pago_del_corte_anterior = en . nextDouble ();
        en . línea siguiente();
        Sistema . fuera . print ( " Ingresa el valor de saldo anterior: " );
        saldo_anterior = en . nextDouble ();
        en . línea siguiente();
        si (saldo_anterior * 0.15 > pago_del_corte_anterior)
        {
            intereses = saldo_anterior * 0.12 ;
            multa = 200 ;
        }
        demás
        {
            intereses = 0 ;
            multa = 0 ;
        }
        saldo_actual = saldo_anterior + compras_realizadas - pago_del_corte_anterior + intereses + multa;
        pago_minimo = saldo_actual * 0.15 ;
        pago_para_no_generar_intereses = saldo_actual * 0.85 ;
        Sistema . fuera . println ( " Valor de intereses: "  + intereses);
        Sistema . fuera . println ( " Valor de multa: "  + multa);
        Sistema . fuera . println ( " Valor de pago mínimo: "  + pago_minimo);
        Sistema . fuera . println ( " Valor de pago para no generar intereses: "  + pago_para_no_generar_intereses);
        Sistema . fuera . println ( " Valor de saldo actual: "  + saldo_actual);
    }
    }

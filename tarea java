 clase  pública Trabajadorde_Coca_Cola {

    public  static  void  main ( String [] args ) {
        Escáner en =  nuevo  escáner ( Sistema . En);
        doble hora_trabajada, horas_extras_diurnas, horas_extras_nocturnas, pensión, salud;
        doble sueldo_basico, sueldo_bruto, sueldo_neto, transporte;
        Sistema . fuera . print ( " Ingresa el valor de hora trabajada: " );
        hora_trabajada = en . nextDouble ();
        en . línea siguiente();
        Sistema . fuera . print ( " Ingresa el valor de horas extras diurnas: " );
        horas_extras_diurnas = en . nextDouble ();
        en . línea siguiente();
        Sistema . fuera . print ( " Ingresa el valor de horas extras nocturnas: " );
        horas_extras_nocturnas = en . nextDouble ();
        en . línea siguiente();
        Sistema . fuera . print ( " Ingresa el valor de sueldo basico: " );
        sueldo_basico = en . nextDouble ();
        en . línea siguiente();
        sueldo_bruto = sueldo_basico + horas_extras_diurnas * hora_trabajada * 1.04 + horas_extras_nocturnas * hora_trabajada * 1.06 ;
        salud = sueldo_bruto * 0.02 ;
        pensión = sueldo_bruto * 0.04 ;
        transporte = sueldo_bruto * 0.03 ;
        sueldo_neto = sueldo_bruto - salud + pensión + transporte;
        Sistema . fuera . println ( " Valor de pensión: "  + pensión);
        Sistema . fuera . println ( " Valor de salud: "  + salud);
        Sistema . fuera . println ( " Valor de sueldo bruto: "  + sueldo_bruto);
        Sistema . fuera . println ( " Valor de sueldo neto: "  + sueldo_neto);
        Sistema . fuera . println ( " Valor de transporte: "  + transporte);




